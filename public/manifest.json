{
  "short_name": "Quick Command",
  "name": "Quick Command",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "extension/build/index.html",
    "default_icon": {
      "16": "/public/favicon.ico"
    }
  },
  "icons": {
    "16": "/public/favicon.ico"
  }
}